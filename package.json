{
  "name": "eater-of-worlds",
  "version": "0.0.1",
  "description": "Ingests Obsidian markdown vaults into Foundry VTT journal pages.",
  "main": "index.js",
  "scripts": {
    "build": "tsc && node utils/build.mjs",
    "build:watch": "concurrently \"tsc --watch\" \"node utils/build.mjs --watch\"",
    "dev:link": "node utils/link.mjs",
    "dev:unlink": "node utils/unlink.mjs",
    "dev": "npm run build && npm run dev:link && npm run build:watch",
    "start": "node dist/index.js",
    "watch": "nodemon --watch src --exec ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuamgrosser/eater-of-worlds.git"
  },
  "keywords": [],
  "author": "Joshua Grosser",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joshuamgrosser/eater-of-worlds/issues"
  },
  "homepage": "https://github.com/joshuamgrosser/eater-of-worlds#readme",
  "devDependencies": {
    "@types/fvtt-types": "github:League-of-Foundry-Developers/foundry-vtt-types#main",
    "@types/node": "^25.0.3",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
